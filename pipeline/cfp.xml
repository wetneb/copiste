<pipeline overlapping="0.5" chunk_size="1024">
	<feature name="spectrum"
		type="Spectrum"
        used="no" />
    <filter name="specreduced"
        type="Range"
        blocksize="8"
        start="0"
        end="512"
        used="no"
        draw="colors">
        <ref name="spectrum" />
    </filter>
    <filter name="fingerprint"
        type="Memory"
        depth="5"
        chunks="16"
        used="yes"
        draw="colors">
        <ref name="specreduced" />
    </filter>
    <filter name="Diff"
        type="Distance"
        profile="mfp2"
        draw="colors"
        used="no">
        <ref name="fingerprint" />
    </filter>  
</pipeline>
