<pipeline>
	<feature type="Spectrum"
		 name="_spectrum"
		 used="no" />

	<!-- Bands -->
	<filter type="Range"
        used="no"
		name="_0_128"
		start="0"
		end="128">
		<ref name="_spectrum" />
	</filter>
	<filter type="Range"
        used="no"
		name="_128_256"
		start="128"
		end="256">
		<ref name="_spectrum" />
	</filter>
	<filter type="Range"
        used="no"
		name="_256_384"
		start="256"
		end="384">
		<ref name="_spectrum" />
	</filter>

	<filter type="Range"
        used="no"
		name="_384_512"
		start="384"
		end="512">
		<ref name="_spectrum" />
	</filter>
	<filter type="Range"
        used="no"
		name="_512_640"
		start="512"
		end="640" >
		<ref name="_spectrum" />
	</filter>
	<filter type="Range"
        used="no"
		name="_640_768"
		start="640"
		end="768" >
		<ref name="_spectrum" />
	</filter>
	 <filter type="Range"
        used="no"
		name="_768_896"
		start="768"
		end="896" >
		<ref name="_spectrum" />
	</filter>
    <filter type="Range"
        used="no"
		name="_896_1024"
		start="896"
		end="1024" >
		<ref name="_spectrum" />
	</filter>
    <!-- Memories -->
    
    <filter type="Memory"
        name="Band 0 - 128"
        depth="5">
        <ref name="_0_128" />
    </filter> 
    <filter type="Memory"
        name="Band 128 - 256"
        depth="5">
        <ref name="_128_256" />
    </filter>
    <filter type="Memory"
        name="Band 256 - 384"
        depth="5">
        <ref name="_256_384" />
    </filter>
    <filter type="Memory"
        name="Band 384 - 512"
        depth="5">
        <ref name="_384_512" />
    </filter>
    <filter type="Memory"
        name="Band 512 - 640"
        depth="5">
        <ref name="_512_640" />
    </filter>
    <filter type="Memory"
        name="Band 640 - 768"
        depth="5">
        <ref name="_640_768" />
    </filter>
    <filter type="Memory"
        name="Band 0 - 128"
        depth="5">
        <ref name="_768_896" />
    </filter>
    <filter type="Memory"
        name="Band 0 - 128"
        depth="5">
        <ref name="_896_1024" />
    </filter>
</pipeline>
